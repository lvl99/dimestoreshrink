{# Layout #}
<!DOCTYPE html>
<html>
  <head>
    <title>{% if isDiagnosis %}{{ diagnosis.output.name }} | {% endif %}Dime Store Shrink</title>
    <meta name="description" content="Get assessed by Dr. Hugh Schtinck, the world's cheapest psychoanalyst. But what price can you put on 'piece of mind' anyway?" />
    <link rel="shortcut icon" type="image/png" href="{{ site.url }}images/favicon.png" />
    <!-- Facebook OG -->
    <meta property="og:site_name" content="Dime Store Shrink" />
    {% if isDiagnosis %}
    <meta property="og:title" content="Dime Store Shrink says I've got &quot;{{ diagnosis.output.name|escape('html_attr') }}&quot;" />
    {% else %}
    <meta property="og:title" content="Dime Store Shrink" />
    {% endif %}
    <meta property="og:description" content="Get assessed by Dr. Hugh Schtinck, the world's cheapest psychoanalyst. But what price can you put on 'piece of mind' anyway?" />
    <meta property="og:type" content="website" />
    {% block extendOGTags %}
      <meta property="og:url" content="{{ site.canonical }}" />
      <link rel="canonical" href="{{ site.canonical }}" />
    {% endblock %}
    <meta property="og:image" content="{{ site.canonical }}images/fbog.png" />
    <!-- Styles -->
    <style type="text/css">
    <!--
    @import url('//fonts.googleapis.com/css?family=Montserrat');
    @import url('//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css');

    body {
      padding: 0;
      margin: 0;
      background: #12a8ff;
      color: #fafafa;
      font-family: 'Montserrat', 'Helvetica Neue', Helvetica, Arial, sans-serif;
      font-size: 24px;
      line-height: 1.4em;
    }

    a:link,
    a:visited {
      color: #eeee40;
      text-decoration: none;
    }

    a:hover,
    a:focus {
      color: #ffff00;
    }

    a:active {
      color: #bff4ff;
    }

    h1, h2, h3, h4, h5, h6 {
      color: #ffffff;
      font-family: 'Montserrat', 'Helvetica Neue', Helvetica, Arial, sans-serif;
    }

    .fa {
      font-family: 'FontAwesome';
    }

    .fa span {
      display: none;
      font-family: 'Montserrat', 'Helvetica Neue', Helvetica, Arial, sans-serif;
    }

    .wrap {
      width: 100%;
      min-width: 240px;
      max-width: 960px;
      margin: 0;
    }

    .columns {
      clear: both;
      overflow: hidden;
    }

    .columns:before,
    .columns:after {
      content: ' ';
      display: table;
      clear: both;
      zoom: 1;
    }

    .columns .column {
      width: 50%;
      float: right;
    }

    .site {
      padding: 30px;
    }

    .site-header {
      margin: 0 0 1.5em 0;
      line-height: 1em;
    }

    .site-header h1 {
      display: inline;
      padding: 0;
      margin: 0;
      font-size: .7em;
      font-weight: bold;
      line-height: 1em;
    }

    .site-header p {
      display: inline;
      padding: 0;
      margin: 0;
      color: #a3dfff;
      font-size: .7em;
      font-weight: normal;
      line-height: 1em;
    }

    .promote {
      display: block;
      position: absolute;
      right: 20px;
      top: 0;
      z-index: 9999;
    }

    .tweet-button {
      display: inline-block;
      line-height: 33px;
    }

    .tweet-button a {
      font-size: .5em;
    }

    .tweet-button iframe {
      vertical-align: bottom;
    }

    .site-footer {
      color: #a3dfff;
      font-size: .7em;
      text-align: right;
    }

    .site-footer a:link,
    .site-footer a:visited {
      color: #c8e6a0;
    }

    .site-footer a:hover,
    .site-footer a:focus {
      color: #ffff00;
    }

    .site-footer a:active {
      color: #bff4ff;
    }

    .diagnosis {
      margin: 0 0 1.5em 0;
    }

    .diagnosis-subtitle {
      font-size: 1.3em;
      line-height: 1.2em;
      padding: 0;
      margin: 0 0 .8em 0;
    }

    .diagnosis-title {
      font-size: 2em;
      line-height: 1.1em;
      padding: 0;
      margin: 0 0 .5em 0;
    }

    .diagnosis-content {
      font-size: 1.2em;
      line-height: 1.2em;
      padding: 0;
      margin: 0 0 1em 0;
    }

    .diagnosis-menu {
      font-size: 1.2em;
    }

    .diagnosis-menu a {
      display: block;
      line-height: 1.2em;
      margin-bottom: .5em;
    }

    .diagnosis-share {
      font-size: 1.6em;
      text-align: right;
      margin: 0 0 .7em 0;
    }

    .diagnosis-share a {
      padding-left: .5em;
    }

    .secondary {
      font-size: .7em;
      line-height: 1.4em;
    }

    .msg-of-love {
      color: #a3dfff;
    }

    @media screen and (max-width: 768px) {
      body {
        font-size: 20px;
      }

      .columns .column {
        width: 100%;
        float: none;
        clear: both;
      }

      .diagnosis-share {
        text-align: left;
      }

      .diagnosis-share a {
        padding: 0 .5em 0 0;
      }
    }
    -->
    </style>
    {% if env == 'development' %}
      <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/9.3.0/highlight.min.js"></script>
      <link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/9.3.0/styles/monokai_sublime.min.css">
      <style type="text/css">
        <!--
        pre code {
        
        }
        -->
      </style>
    {% endif %}
    {% block extendHead %}{% endblock %}
    {% if site.ga is defined and site.ga is not empty %}
      <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
        ga('create', {{ site.ga }}, 'auto');
        ga('send', 'pageview');
      </script>
    {% endif %}
  </head>
  <body>
    <section class="site">

      <div class="promote">
        <div class="tweet-button">
          <a href="https://twitter.com/intent/tweet?screen_name=dimestoreshrink&ref_src=twsrc%5Etfw" class="twitter-mention-button" data-show-count="false" data-url="{{ site.canonical }}">
            Tweet @dimestoreshrink
          </a>
          <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
        </div>
      </div><!-- /.promote -->

      <div class="wrap">
        <header class="site-header">
          <h1><a href="/">Dr Hugh Schtinck, Dime Store Shrink</a></h1>
          <p>{{ funnyHeader }}</p>
        </header><!-- /.site-header -->

        <div role="main" class="site-content">
          {% block content %}
            <h2>Welcome!</h2>
            <big><p>I'm Dr. Hugh Schtinck, your friendly internet psychiatrist!</p></big>
            <p>I'm the world's cheapest psychoanalyst. But what price can you put on "piece of mind" anyway?</p>
            <p><a href="{{ site.url }}">Tell me what I got, doc!</a></p>
          {% endblock %}
        </div><!-- /.site-content -->

        <footer class="site-footer">
          <p>Created for fun by <a href="http://www.lvl99.com/" target="_blank">LVL99</a></p>
        </footer><!-- /.site-footer -->

      </div><!-- /.wrap -->
    </section><!-- /.site -->
  </body>
</html>
